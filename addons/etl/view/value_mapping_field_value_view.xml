        <record id="view_etl_value_mapping_field_value_filter" model="ir.ui.view">
            <field name="name">etl.value_mapping_field_value.select</field>
            <field name="model">etl.value_mapping_field_value</field>
            <field name="arch" type="xml">
                <search string="value_mapping_field_value" version="7.0">
                    <group string="By Attribute">
                    <field name="ext_id" string="Key"
                            />
                    <field name="name" string="Help Name"
                            />
                    </group>
                    <group string="By object">
                    </group>
                </search>
            </field>
        </record>

        <!-- FORMVIEW -->
        <record id="view_etl_value_mapping_field_value_form" model="ir.ui.view">
            <field name="name">etl.value_mapping_field_value.form</field>
            <field name="model">etl.value_mapping_field_value</field>
            <field name="arch" type="xml">
                <form string="value_mapping_field_value" version="7.0">
                    <header>
                    </header>
                    <sheet string="value_mapping_field_value">
                     <group>
                        <field name="ext_id"
                            />
                        <field name="name"
                            />
                    <field name="value_mapping_field_id"
                            />
                     </group>
                    </sheet>

                </form>
            </field>
        </record>

        <!-- TREEVIEW -->

        <record id="view_etl_value_mapping_field_value_tree" model="ir.ui.view">
            <field name="name">etl.value_mapping_field_value.tree</field>
            <field name="model">etl.value_mapping_field_value</field>
            <field name="arch" type="xml">
                <tree string="value_mapping_field_value"
                    >
                    <field name="ext_id"
                        />
                    <field name="name"
                        />
                    <field name="value_mapping_field_id"
                        />
                </tree>
            </field>
        </record>

    </data>
</openerp>
