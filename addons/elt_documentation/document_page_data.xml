            <field name="content">
<![CDATA[
<iframe src="https://docs.google.com/document/d/1HWERCUp9rMHqEibT7B_mfu9jePV-FBkYbo--OB0jXqA/pub" width="100%" height="95%" frameborder="0"></iframe>
]]>
            </field>
        </record>

        <record id="action_elt_page" model="ir.actions.act_window">
            <field name="name">odoo ELT</field>
            <field name="res_model">document.page</field>
            <field name="view_mode">form</field>
            <field name="target">inlineview</field>
            <field name="res_id" eval="str(ref('elt_page'))"/> 
        </record>
                
        <menuitem id="menu_elt_page" sequence="10"
                  action="action_elt_page"
                  parent="menu_elt_documentation"/>

                  
    </data>
</openerp>
